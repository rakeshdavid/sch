    <token name="T_WHITESPACE"> </token>
    <token name="T_CLOSE_TAG">?&gt;</token>
  </line>
  <line no="29">
    <token name="T_INLINE_HTML">&lt;h2&gt;Setting slider values&lt;/h2&gt;</token>
  </line>
  <line no="30"/>
  <line no="31">
