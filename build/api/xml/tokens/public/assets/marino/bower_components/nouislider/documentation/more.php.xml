  <line no="120">
    <token name="T_INLINE_HTML">&lt;h2&gt;Styling&lt;/h2&gt;</token>
  </line>
  <line no="121"/>
  <line no="122">
    <token name="T_INLINE_HTML">&lt;section&gt;</token>
