  <line no="9">
    <token name="T_INLINE_HTML">&lt;h2&gt;Disabling a slider&lt;/h2&gt;</token>
  </line>
  <line no="10"/>
  <line no="11">
    <token name="T_INLINE_HTML">&lt;section&gt;</token>
  </line>
